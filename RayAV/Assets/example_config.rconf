enable air absorption=true
reference materials directory=Assets/rayav_assets/materials/ReferenceMaterials/
materials directory=Assets/rayav_assets/materials/ReferenceMaterials/
use default instead of inaccessible material=true
enable doppler effect=false
SPL threshold=-60 #dB max value: 0 dB
RMS difference between parent and child diffraction beam = -3; # dB.
BQC_HRTF=Assets/rayav_assets/hrtf/bqc_hrtf.dat
diffraction table path=Assets/rayav_assets/diffraction_tables/diffraction_table_24_8.dat
use interpolated diffraction=true
use headphones=true
#------------------------------------------------------

import Assets/rayav_assets/reverb_off.txt

#Pipelines configuration
import Assets/rayav_assets/pipelinesconfig.txt

#Compressor
import Assets/rayav_assets/compressorconfig.txt