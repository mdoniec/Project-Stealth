#--------------Scene file-----------------------------
scene file=Assets/MAPS/AcuMap.rmf

#------------------Geometry options--------------------
adfrustum accuracy=4
use varying beam accuracy=false
adfrustum generation 0 accuracy=4
adfrustum generation 1 accuracy=3
adfrustum generation 2 accuracy=3
adrayfrustum generation 0 preprocessing level=1
adrayfrustum generation 1 preprocessing level=0
adrayfrustum generation 2 preprocessing level=0
# WARNING!
# for SSE version, [adrayfrustum preprocessing level] must be at most equal to [adfrustum accuracy]-2
#      for AVX version, [adrayfrustum preprocessing level] must be at most equal to [adfrustum accuracy]-3
adrayfrustum preprocessing level=1

max beam generation=3

## common options ##
enable beam merging=true
enable beam reflection=true
enable beam transmission=true
enable beam diffraction=true
max geometry tree depth=10
trigger triangle count for geometry tree cell subdivision=10

enable sound processing=true
use triangle based beams=true
